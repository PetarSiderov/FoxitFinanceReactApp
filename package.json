{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@trendyol-js/react-carousel": "^2.0.0",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-elastic-carousel": "^0.11.5",
    "react-id-swiper": "^4.0.0",
    "react-responsive-carousel": "^3.2.22",
    "react-responsive-carousel-2": "^3.2.19",
    "react-scripts": "4.0.3",
    "react-simple-image-slider": "^2.3.0",
    "styled-components": "^5.3.3",
    "swiper": "^7.3.1",
    "tiny-slider-react": "^0.5.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "compilerOptions": {
    "strict": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [
      "node",
      "jest"
    ],
    "newLine": "LF",
    "outDir": "lib",
    "target": "es2017",
    "sourceMap": true,
    "declaration": true,
    "jsx": "preserve",
    "lib": [
      "es2017",
      "dom"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "baseUrl": "./src",
    "paths": {
      "@client/*": [
        "./client/*"
      ],
      "@server/*": [
        "./server/*"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    ".git",
    "node_modules"
  ]
}
